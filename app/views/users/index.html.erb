<tr>
    <th>名前</th>
  </tr>
  <% @users.each do |user| %> 
    <tr>
      <td><%= user.name %></td>
      <td><%= link_to '詳細', user %></td>
      <td><%= link_to '編集', edit_user_path(user.id) %></td>
      <td><%= link_to '削除', task_path(user.id), method: :delete, data: { confirm: '本当に削除していいですか？' } %></td>
    </tr>
  <% end %>